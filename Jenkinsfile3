pipeline {
    agent any

    stages {
        stage('GITHUB') {
            steps {
                //echo 'Hello World'
               // https://github.com/devopsdecember/java-tomcat-maven-example.git
               git credentialsId: 'githublogin', url: 'https://github.com/devopsdecember/java-tomcat-maven-example.git'
            }
        }
         stage('Maven Build') {
            steps {
                bat 'mvn clean install'
            }
        }
    }
}
