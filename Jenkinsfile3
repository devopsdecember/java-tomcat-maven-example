pipeline {
    agent any

    stages {
           stage('Maven Build') {
            steps {
                bat 'mvn clean install'
            }
        }
stage('Build') {
            steps {
                bat 'mvn clean install'
            }
        }
stage('Archive') {
            steps {
                archiveArtifacts artifacts: '**/*.war', followSymlinks: false
            }
        }
    }
}
